{
   "deno.enable": true,
   "editor.defaultFormatter": "denoland.vscode-deno",
   "prettier.enable": false,
   "eslint.enable": false,
   "editor.formatOnSave": true,
   "[javascript][typescript][javascriptreact][typescriptreact]": {
      "editor.defaultFormatter": "denoland.vscode-deno"
   },
   "[markdown][json][jsonc][css][html][yaml]": {
      "editor.defaultFormatter": "denoland.vscode-deno"
   }
}
